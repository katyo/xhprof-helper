{
  "name": "XHProf Interface",
  "description": "Helper for profiling PHP apps with XHProf",
  "version": "0.1",
  "icons": {
    "128": "icon_128.png",
    "64": "icon_64.png",
    "48": "icon_48.png",
    "32": "icon_32.png",
    "16": "icon_16.png"
  },
  "permissions": [
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "http://*/*",
    "https://*/*"
  ],
  "options_page": "options.html",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "page_action": {
    "default_icon": "icon_19.png",
    "default_title": "XHProf",
    "default_popup": "popup.html",
    "popup": "popup.html"
  },
  "manifest_version": 2
}
